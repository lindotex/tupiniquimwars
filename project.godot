[application]

config/name="Tupiniquim Wars"
config/description="An exciting game about the fight of the indigenous people of Pindorama, trying to expell the Portuguese colonizers from their land. Portuguese empire are trying to stabilish their \"colony\", changing our lands' name to Brazil. Help our Hero on this EPIC battle.	"
run/main_scene="res://Scenes/menus/menu.tscn"
